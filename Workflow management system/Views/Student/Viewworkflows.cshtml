@model IEnumerable<Workflow_management_system.Models.WorkflowType>

@{
    ViewBag.Title = "Viewworkflows";
}

<h2>View workflows</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Create Request", "CreateWorkflow", new { id=item.WorkflowTypeID }) |
        </td>
    </tr>
}

</table>
